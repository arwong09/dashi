{
  "extends": [
    "eslint-config-airbnb-base",
    "airbnb/rules/react",
    "airbnb/hooks",
    "next/core-web-vitals"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".tsx"] }],
    "react/function-component-definition": [0],
    "react/jsx-props-no-spreading": [0],
    "semi": [2, "never"],
    "import/extensions": [0],
    "react/require-default-props": [0]
  }
}
